<Page navigatedTo="enter" >
    <Page.actionBar>
        <ActionBar title="Title">
            <ActionBar.titleView>
                <StackLayout orientation="horizontal">
                    <Button text="1st" />
                    <Button text="2nd" />
                    <Button text="3rd" />
                </StackLayout>
            </ActionBar.titleView>
        </ActionBar>
    </Page.actionBar>
    <StackLayout>
            <Label text="{{name}}" tap="{{tapAction}}"/>
            <Button text="Take Photo" tap="{{openCamera}}"/>
            <Image src="{{imgSrc}}" />
    </StackLayout>
</Page>